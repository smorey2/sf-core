/**
* a tuple with three items
*/
global class Tuple3 {
    global final Object item1;
    global final Object item2;
    global final Object item3;
    global Tuple3(Object item1, Object item2, Object item3) {
        this.item1 = item1;
        this.item2 = item2;
        this.item3 = item3;
    }
}