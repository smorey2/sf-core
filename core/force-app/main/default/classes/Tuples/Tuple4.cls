/**
* a tuple with four items
*/
global class Tuple4 {
    global final Object item1;
    global final Object item2;
    global final Object item3;
    global final Object item4;
    global Tuple4(Object item1, Object item2, Object item3, Object item4) {
        this.item1 = item1;
        this.item2 = item2;
        this.item3 = item3;
        this.item4 = item4;
    }
}